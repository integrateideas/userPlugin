{
    "name": "Integrateideas/User",
    "description": "Integrateideas/User plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Integrateideas\\User\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Integrateideas\\User\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
